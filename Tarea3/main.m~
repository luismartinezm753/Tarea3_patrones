%%Main de la tarea
descriptors=load('Rand_descriptors.mat');
centers=cell(1,7);
k=1;
data=descriptors.data;
%creamos los cluster
for i=[100 300 500 700 900 1100 1300]
    if exist('')
        continue
    end
    centers{k}=clustering(i,data); %la traspuesta
    k=k+1;
end
pathTrain='Dataset/Train';
k=100;
trainer=cell(1,7);
%entrenamos el SVM
for j=1:numel(centers)
    trainer{j}=trainer(pathTrain,centers{j});
    saveTrainer(trainer{j},k);
    k=k+200;
end

